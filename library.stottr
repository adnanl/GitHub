@prefix dc:   <http://purl.org/dc/elements/1.1/> .
@prefix dbpo: <http://dbpedia.org/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo:  <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ottr: <http://ns.ottr.xyz/0.4/> .
@prefix o-rdf: <http://tpl.ottr.xyz/rdf/0.1/> .
@prefix ex: <http://example.com/ns#> .

ex:Person[ottr:IRI ?personIRI, ! xsd:string ?name ] :: { 
  ottr:Triple(?personIRI, rdf :type, foaf:Person), 
  ottr:Triple(?personIRI, foaf:name, ?name) 
  }.

ex:Point[ottr:IRI ?locationIRI, ! xsd:decimal ?lat, ! xsd:decimal ?long, ?! xsd:string ?location] ::{
  ottr:Triple(?locataionIRI, rdf:type, geo:Point)
  ottr:Triple(?locataionIRI, geo:lat, ?lat)
  ottr:Triple (?locationIRI, geo:long, ?long)
  orrt:Triple (?locationIRI, dc:description, ?location)
  }.

ex:Sculpture [ ! xsd:decimal ?lat, ! xsd:decimal ?long, ?! xsd:string ?location, ! xsd:string ?title, ?! xsd:string ?artistName, ? xsd:date ?year ] ::{
  ottr:Triple(?sculpture rdf:type dbpo:Sculpture),
  ex:Point(ottr:IRI ?locationIRI, ! xsd:decimal ?lat, ! xsd:decimal ?long, ?! xsd:string ?location), ottr:Triple(?sculpture dc:title ?title)
  ex:Person(ottr:IRI ?personIRI, ! xsd:string ?name), ottr:Triple(?sculpture dc:date ?date) 
  }  .

